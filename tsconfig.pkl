//amends "package://pkg.pkl-lang.org/github.com/PaulJPhilp/pklTsConfig/pklTsConfig@0.5.1#/TsConfigDefault.pkl"
amends "./src/default.pkl"

totallyTypescriptConfigurations {
  "Strictest"
  "NotDom"
}

`extends` = "project_tsconfig.json"
files {
    "main.ts"
    "index.ts"
    }
exclude {
    "exlude.ts"
    }
include {
    "include.ts"
}

compilerOptions {
    strictNullChecks = true
    allowJs = false
}

outputFileName = "tsconfig.json"