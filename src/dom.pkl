module Dom

//typealias DomItems = Mapping<String, String|Boolean>


class DomItems {
    strict: Boolean
    noUncheckedIndexedAccess: Boolean
}


const Dom = new  DomItems{
    strict = true
    noUncheckedIndexedAccess = true
}

function addDomItems(domItems: DomItems, useDom: Boolean): Map = 
    if (useDom) 
        new  DomItems {
            strict = domItems.strict
            noUncheckedIndexedAccess =  domItems.noUncheckedIndexedAccess
        }.toMap()
    else
        Map()
